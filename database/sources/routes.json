{
  "root": [
    {
      "domain": "root",
      "name": "drive.local.serve",
      "pattern": "/uploads/*",
      "methods": [
        "GET",
        "HEAD"
      ],
      "handler": "Closure",
      "middleware": []
    },
    {
      "domain": "root",
      "name": "routes.index",
      "pattern": "/api/routes",
      "methods": [
        "GET",
        "HEAD"
      ],
      "handler": "RoutesController.index",
      "middleware": []
    },
    {
      "domain": "root",
      "name": "routes.create",
      "pattern": "/api/routes/create",
      "methods": [
        "GET",
        "HEAD"
      ],
      "handler": "RoutesController.create",
      "middleware": []
    },
    {
      "domain": "root",
      "name": "routes.update",
      "pattern": "/api/routes/:id",
      "methods": [
        "PUT",
        "PATCH"
      ],
      "handler": "RoutesController.update",
      "middleware": []
    }
  ]
}
